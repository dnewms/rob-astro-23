# Rule for students
RewriteCond %{REQUEST_URI} ^/profile/(ray-zhang|xiaotong-chen|peter-mitrano|alex-manohar|daphna-raz|neha-pusalkar|ruikun-luo|shengao-wang|akshay-mathur|marcela-de-los-rios|nikhil-devraj|harsh-jhaveri|isha-bhatt|onur-bagoren|emma-reznick|mingjie-bi|rui-chen|shreya-phirke|oluwami-wami-dosunmuogunbi|thirumalaesh-ashokkumar|siddharth-anilkumar-bhurat|aayushi-shrivastava|manohar-bhat|ke-liu|zheyuan-zhang|deepak-nagalla|peter-stratton|tzuyuan-justin-lin|chien-erh-cynthia-lin|joseph-kim|ziyou-wu|kajal-awasthi|vinay-angara|soham-sachin-purohit|saket-pradhan|trushant-adeshara|ye-li|miranda-mittleman|trinh-huynh-trinh-huynh|christopher-nesler|xiangyu-peng|mark-nail|andrea-sipos|nishant-kheterpal|alphonsus-adubredu|challen-enninful-adu|ross-cortino|luis-cubillos|chenghao-lin|brandon-apodaca|mohamad-louai-shehab|advaith-sethuraman|katharine-walters|anja-sheppard|lus-marques|shambhavi-singh|yulun-zhuang|xiaohao-xu|patrick-tosh|mudasir-ahmad-ganaie|m-eva-mungai|alia-gilbert|bo-fu|grant-gibson)/?$
RewriteRule ^profile/(.+)$ /people/students [R=301,L]

# Rule for news:
RewriteRule ^(\d{4})/(.+)$ /news/$1/$2 [R=301,L]

# Rule for media:
RewriteRule ^about/media/?$ /about/contact [R=301,L]

# Rule for datasets:
RewriteRule ^research/datasets/?$ /research/projects-and-datasets [R=301,L]

# Redirect any non-existent profile pages to the main people directory:
RewriteCond %{REQUEST_URI} ^/profile/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /people [R=301,L]

# Redirect any non-existent news pages to the main news directory:
RewriteCond %{REQUEST_URI} ^/news/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /news [R=301,L]